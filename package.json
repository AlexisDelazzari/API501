{
  "name": "typeorm-sample",
  "version": "1.0.2",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "release-it": "^17.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "typeorm": "0.3.17"
  },
  "scripts": {
    "typeorm": "typeorm-ts-node-commonjs",
    "dev": "ts-node-dev src/index.ts",
    "test": "jest --watch --coverage",
    "typeorm:migrate": "npx typeorm-ts-node-esm migration:run -d src/config/ormconfig.ts",
    "typeorm:generate": "npx typeorm-ts-node-esm migration:generate  -d src/config/ormconfig.ts",
    "typeorm:revert": "npx typeorm-ts-node-esm migration:revert -d src/config/ormconfig.ts",
    "typeorm:show": "npx typeorm-ts-node-esm migration:show -d src/config/ormconfig.ts",
    "typeorm:sync": "npx typeorm-ts-node-esm schema:sync -d src/config/ormconfig.ts",
    "typeorm:drop": "npx typeorm-ts-node-esm schema:drop -d src/config/ormconfig.ts",
    "typeorm:run": "npx typeorm-ts-node-esm migration:run -d src/config/ormconfig.ts",
    "release": "release-it"
  }
}
